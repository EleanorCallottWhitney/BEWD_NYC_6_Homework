<p> Welcome to InstaFlix! See our collection of movies below! </p>

<%= link_to "Add", new_movie_path %>

<table>
	<tr>
		<th>title</th>
		<th>description</th>
		<th>year_released</th>
	</tr>		
 
<% @movies.each do |movie| %> 

<tr>
	<td><%= link_to movie.title, movie_path(movie) %></td>
	<td><%= movie.description %></td>
	<td><%= movie.year_released %></td>
</tr>

<% end %>

</table>
